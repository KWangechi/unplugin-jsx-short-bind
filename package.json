{
  "name": "unplugin-jsx-short-bind",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.12.0",
  "description": "JSX short bind.",
  "author": "zhiyuanzmj",
  "license": "MIT",
  "homepage": "https://github.com/zhiyuanzmj/unplugin-jsx-short-bind#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhiyuanzmj/unplugin-jsx-short-bind.git"
  },
  "bugs": {
    "url": "https://github.com/zhiyuanzmj/unplugin-jsx-short-bind/issues"
  },
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./volar": {
      "import": "./dist/volar.js",
      "require": "./dist/volar.cjs"
    },
    "./eslint": {
      "import": "./dist/eslint.js",
      "require": "./dist/eslint.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "play": "pnpm run -C ./playground dev",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@ast-grep/napi": "^0.16.0",
    "@vue-macros/common": "^1.10.0",
    "@vue/language-core": "^1.8.27",
    "unplugin": "^1.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@babel/types": "^7.23.6",
    "@types/node": "^20.10.6",
    "@vue-macros/test-utils": "^1.3.0",
    "@vue-macros/volar": "^0.18.8",
    "bumpp": "^9.2.1",
    "change-case": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unplugin-vue-macros": "^2.7.7",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue": "^3.4.3"
  }
}
